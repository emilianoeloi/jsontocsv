{
  "manifest_version": 2,
  "name": "JSON to CSV",
  "description": "Convert JSON to CSV",
  "version": "1",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "128": "assets/img/logo_128.png",
    "16": "assets/img/logo_16.png"
  },
  "offline_enabled" : true,
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "background"
  ]
}
